#include <stdio.h>

int main() {
  int time = 22;
  if (time < 10) {
    printf("Good morning.");
  } else if (time < 20) {
    printf("Good day.");
  } else {
    printf("Good night.");   //good night
  }
  return 0;
}


#include <stdio.h>

int main() {
  if (2>1) {
    printf("2 is greater than 1");  //2  is greater than 1
  }  
  return 0;
}


#include <stdio.h>

int main() {
  int myNum = 10;
  
  if (myNum > 0) {
    printf(" positive number.");  //positive number
  } else if (myNum < 0) {
    printf("negative number.");
  } else {
    printf("The value is 0.");
  }
  
  return 0;
}
