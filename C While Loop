#include <stdio.h>

int main() {
  int i = -5;
  
  while (i < 5) {
    printf("%d\n", i);   //output-5 -4 -3 -2 -1 0 1 2 3 4
    i++;
  }
  
  return 0;
}


#include <stdio.h>

int main() {
  int i = -5;
  
  do {
    printf("%d\n", i);  //output -5 -4 -3 -2 -1 0 1 2 3 4 
    i++;
  }
  while (i < 5);
  
  return 0;
}
